{
  "name": "unist-util-ancestor",
  "version": "1.1.1",
  "description": "Unist utility for finding a common ancestor for multiple nodes. Useful for working with remark, rehype and retext.",
  "license": "MIT",
  "keywords": [
    "unist",
    "remark",
    "retext",
    "rehype",
    "hast",
    "mdast",
    "nlcst",
    "find",
    "ancestor",
    "search"
  ],
  "author": "Goran Spasojevic <gospaso@gmail.com> (https://gorango.me)",
  "repository": "gorango/unist-util-ancestor",
  "bugs": "https://github.com/gorango/unist-util-ancestor/issues",
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "build": "rimraf \"{index,test}.d.ts\" && tsc",
    "test": "node test.js",
    "coverage": "npx c8 --check-coverage --branches 100 --functions 100 --lines 100 --statements 100 --reporter lcov node test.js"
  },
  "dependencies": {
    "@types/unist": "^2.0.0",
    "unist-util-visit-parents": "^5.0.0"
  },
  "devDependencies": {
    "@types/tape": "^4.13.2",
    "rimraf": "^3.0.2",
    "tape": "^5.3.1",
    "typescript": "^4.4.3",
    "unist-builder": "^3.0.0"
  }
}